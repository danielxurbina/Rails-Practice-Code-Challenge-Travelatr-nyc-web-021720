<h1>Create A Blogger</h1>

<% if flash[:errors] %>
  <ul>
    <% flash[:errors].each do |error| %>
      <li><%= error %></li>
    <% end %>
  </ul>
<% end %>
</ul>

<%= form_for @blogger do |f| %>
Name: <%= f.text_field :name %>
Bio: <%= f.text_area :bio %>
Age: <%= f.number_field :age %>
<%= f.submit %>
    <%end%>



    

    
    
